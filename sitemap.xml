---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
        xmlns:image="http://www.google.com/schemas/sitemap-image/1.1"
        xmlns:news="http://www.google.com/schemas/sitemap-news/0.9">
  
  <!-- Homepage -->
  <url>
    <loc>{{ site.url }}/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>1.0</priority>
    <image:image>
      <image:loc>{{ site.url }}{{ site.image }}</image:loc>
      <image:caption>Sanket Muchhala - AI Engineer and Data Scientist Portfolio</image:caption>
    </image:image>
  </url>

  <!-- Projects -->
  {% for project in site.projects %}
  <url>
    <loc>{{ site.url }}{{ project.url }}</loc>
    <lastmod>{% if project.date %}{{ project.date | date: "%Y-%m-%d" }}{% else %}{{ site.time | date: "%Y-%m-%d" }}{% endif %}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.8</priority>
    {% if project.image %}
    <image:image>
      <image:loc>{{ site.url }}{{ project.image }}</image:loc>
      <image:caption>{{ project.title }} - AI/ML Project by Sanket Muchhala</image:caption>
    </image:image>
    {% endif %}
  </url>
  {% endfor %}

  <!-- Blogs -->
  {% for blog in site.blogs %}
  <url>
    <loc>{{ site.url }}{{ blog.url }}</loc>
    <lastmod>{% if blog.date %}{{ blog.date | date: "%Y-%m-%d" }}{% else %}{{ site.time | date: "%Y-%m-%d" }}{% endif %}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.7</priority>
    {% if blog.featured_image %}
    <image:image>
      <image:loc>{{ site.url }}{{ blog.featured_image }}</image:loc>
      <image:caption>{{ blog.title }} - Blog by AI Engineer Sanket Muchhala</image:caption>
    </image:image>
    {% endif %}
  </url>
  {% endfor %}

  <!-- Static Pages -->
  <url>
    <loc>{{ site.url }}/projects/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.8</priority>
  </url>

  <url>
    <loc>{{ site.url }}/blogs/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.7</priority>
  </url>

  <url>
    <loc>{{ site.url }}/photography/photos/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.6</priority>
  </url>

  <url>
    <loc>{{ site.url }}/resume/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>monthly</changefreq>
    <priority>0.7</priority>
  </url>

</urlset>